    /// Style 1: Adjective + Noun (Raven Rock, Red Pass, Everwind City)
    repeat 5
        adj  | pick 1 from adjectives
        noun | pick 1 from geo_nouns
        place | adj ++ noun
        say place
    xx
    /// Style 2: Possessive Landmark (King's Landing, Rosebeard Peak)
    repeat 5
        owner | pick 1 from owners
        noun  | pick 1 from geo_nouns
        owner_s | owner + "'s"
        place | "The" ++ owner_s ++ noun
        say place
    xx
    /// Style 3: Region Names (The Shire, The Wilds, The Wastes)
    repeat 4
        region | pick 1 from region_words
        place  | "The" ++ region
        say place
    xx
    /// Style 4: Compound Geo (Overvale, Everwind, Bartertown-style)
    repeat 3
        pre | pick 1 from geo_prefixes
        suf | pick 1 from geo_suffixes
        place | pre + suf
        say place
    xx
    /// Style 5: Invented Fantasy Names (2â€“3 syllables, title-cased)
    repeat 3
        count | pick 1 from 2..3
        parts | pick {count} from name_syllables
        raw_name | :pack(parts)
        place | raw_name.title
        say place
    xx
xx
